{
	"name": "vhdl",
	"displayName": "VHDL",
	"description": "VHDL support for VS Code",
	"version": "0.0.3",
	"publisher": "mshr-h",
	"engines": {
		"vscode": "^0.10.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mshr-h/vscode-vhdl-support.git"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [{
			"id": "vhdl",
			"aliases": ["VHDL", "vhdl"],
			"extensions": [".vhd",".vhdl",".vho"],
			"configuration": "./vhdl.configuration.json"
		}],
		"grammars": [{
			"language": "vhdl",
			"scopeName": "source.vhdl",
			"path": "./syntaxes/vhdl.tmLanguage"
		}],
		"snippets": [
			{
				"language": "vhdl",
				"path": "./snippets/vhdl.json"
			}
		]
	}
}
